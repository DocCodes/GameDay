{
  "name": "GameDay",
  "author": "Evan Young",
  "version": "0.1.0a",
  "description": "A video-game information platform.",
  "keywords": [
    "video-games",
    "games",
    "steam"
  ],
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=builds",
    "pack-win": "electron-packager . gameday --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=builds --version-string.CompanyName=\"Evan Young\" --version-string.FileDescription=\"A video-game information platform\" --version-string.ProductName=\"GameDay\"",
    "pack-tux": "electron-packager . gameday --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=builds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DocCodes/GameDay.git"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-packager": "^10.1.1"
  }
}
