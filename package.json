{
  "name": "GameDay",
  "productName": "GameDay",
  "author": "Evan Young",
  "version": "2.0.1a",
  "description": "A video-game information platform.",
  "keywords": [
    "video-games",
    "games",
    "steam"
  ],
  "main": "main/index.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . gameday --overwrite --asar --platform=darwin --arch=x64  --prune=true --out=builds --app-copyright\"Copyright © 2018 Evan Young\" --app-bundle-id=\"com.evanyoung.gameday\"",
    "build-win": "electron-packager . gameday --overwrite --asar --platform=win32  --arch=ia32 --prune=true --out=builds --app-copyright\"Copyright © 2018 Evan Young\" --win32metadata.CompanyName=\"Evan Young\" --win32metadata.FileDescription=\"A video-game information platform.\" --win32metadata.ProductName=\"GameDay\"",
    "build-lux": "electron-packager . gameday --overwrite --asar --platform=linux  --arch=x64  --prune=true --out=builds --app-copyright\"Copyright © 2018 Evan Young\"",
    "setup-mac": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DocCodes/GameDay.git"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^10.1.1"
  },
  "dependencies": {
    "application-config": "^1.0.1"
  }
}
